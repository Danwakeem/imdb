    "scrape",
    "node",
    "interface"
  ],
  "devDependencies": {
    "@types/cheerio": "^0.22.7",
    "@types/jest": "^23.0.0",
    "@types/node": "^10.5.1",
    "@types/request-promise-native": "^1.0.15",
    "jest": "^22.4.3",
    "ts-jest": "^22.4.6",
    "typescript": "^2.9.1"
