    },
    "testRegex": "(/__tests__/.*|(\\.|/)(test|spec))\\.(jsx?|tsx?)$",
    "moduleFileExtensions": [
      "ts",
      "tsx",
      "js",
      "jsx",
      "json",
      "node"
    ]
  },
  "author": "mhsjlw",
  "license": "MIT",
  "repository": {
    "type": "git",
    "url": "git+https://github.com/mhsjlw/imdb.git"
  },
  "bugs": {
    "url": "https://github.com/mhsjlw/imdb/issues"
  },
  "homepage": "https://github.com/mhsjlw/imdb#readme",
  "keywords": [
