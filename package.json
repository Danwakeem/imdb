{
  "name": "imdb",
  "version": "2.0.0",
  "description": "An IMDb interface for Node",
  "main": "index.js",
  "engines": {
    "node": ">=8"
  },
  "dependencies": {
    "cheerio": "^1.0.0-rc.2",
    "request": "^2.85.0",
    "request-promise-native": "^1.0.5"
  },
  "scripts": {
    "test": "jest"
